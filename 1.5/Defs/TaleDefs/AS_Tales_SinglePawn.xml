  <TaleDef>
    <defName>AS_BloodRefiningBreakthrough</defName>
    <label>blood refining breakthrough</label>
    <taleClass>Tale_SinglePawn</taleClass>
    <type>Volatile</type>
    <baseInterest>2</baseInterest>
    <rulePack>
      <rulesStrings>        
	    <li>tale_noun->[PAWN_nameDef]'s blood refining breakthrough</li>
        <li>image->[PAWN_nameFull] [eat_gerund] blood refining body realm [circumstance_group]</li>
        <li>eat_gerund->made a breakthrough into</li>
        <li>eat_gerund->achieved </li>
        <li>eat_gerund->cultivated into</li>
        <li>circumstance_phrase->with a sense of purpose</li>
		<li>circumstance_phrase->while holding back tears</li>
        <li>circumstance_phrase->while the heavens protested with lightning</li>
        <li>circumstance_phrase->with relative ease</li>
        <li>circumstance_phrase->while others look on</li>
        <li>desc_sentence->[PAWN_nameDef] is seen covered in sweat.</li>
        <li>desc_sentence->[PAWN_nameDef]'s body seems to be at its limits.</li>
      </rulesStrings>
    </rulePack>
  </TaleDef>
  
  
